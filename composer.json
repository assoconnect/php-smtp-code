{
    "name": "assoconnect/php-smtp-code",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/php-smtp-code/"
        }
    ],
    "type": "library",
    "description": "Parser for SMTP codes",
    "keywords": ["php", "smtp"],
    "homepage": "https://github.com/assoconnect/php-smtp-code/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\SmtpCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\SmtpCode\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3",
        "symfony/contracts": "^2.4",
        "assoconnect/phpstan-rules": "^1.0"
    },
    "require": {
        "php": "^7.4|^8.0"
    }
}
